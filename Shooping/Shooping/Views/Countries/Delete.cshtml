@model Shooping.Data.Entities.Country

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Desea eliminar el registro seleccionado</h3>
<div>
    <partial name="_CountryDetails" />
    
    <form asp-action="Eliminar" class="btn btn-outline-warning">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-outline-danger" /> 
        <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
    </form>
</div>
